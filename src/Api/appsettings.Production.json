{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SiteSettings": {
    "JwtSettings": {
      "SecretKey": "LongerThan-16Char-SecretKey-1234544-dasdasdasdasdasdasd",
      "EncryptKey": "16CharEncryptKey",
      "Issuer": "MyWebsite",
      "Audience": "MyWebsite",
